<h1>developers</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Devices</th>
    <th colspan="3"></th>
  </tr>

  <% @developers.each do |developer| %>
    <tr>
      <td><%= developer.name %></td>
      <td><%= developer.description %></td>
      <td><%= developer.devices %></td>
      <td><%= link_to 'Show', developer_path(developer) %></td>
      <td><%= link_to 'Edit', edit_developer_path(developer) %></td>
      <td><%= link_to 'Destroy', developer_path(developer),
              method: :delete,
              data: { confirm: 'Are you sure?' } %>
      </td>
    </tr>
  <% end %>
</table>
