<h1>Phones</h1>

<table>
  <tr>
    <th>Developer</th>
    <th>Manufacturer</th>
    <th>Model</th>
    <th>Description</th>
    <th>Year</th>
    <th>Resolution</th>
    <th>Diagonal</th>
    <th>Screen type</th>
    <th>DPI</th>
    <th>Multitouch</th>
    <th>CPU</th>
    <th>Cores</th>
    <th>Frequency</th>
    <th>GPU</th>
    <th>Inner memory</th>
    <th>Access memory</th>
    <th>RAM</th>
    <th>Camera</th>
    <th>Megapixels</th>
    <th>Photo resolution</th>
    <th>HDR</th>
    <th>Front megapixels</th>
    <th>Video resolution</th>
    <th>FPS</th>
    <th>4K video</th>
    <th>3G</th>
    <th>4G</th>
    <th>WiFi</th>
    <th>Bluetooth</th>
    <th>NFC</th>
    <th>GPS</th>
    <th>Gyroscope</th>
    <th>Accelerometer</th>
    <th>Barometer</th>
    <th>Wireless charge</th>
    <th>Fast charge</th>
    <th>VR</th>
    <th>Battery</th>
    <th>Standby time</th>
    <th>Talk time</th>
    <th>OS</th>
    <th>Color</th>
    <th>Weight</th>
    <th>Dimensions</th>
    <th>Material</th>
    <th>Protection</th>
  </tr>

  <% @phones.each do |phone| %>
    <tr>
      <td><%= phone.developer %></td>
      <td><%= phone.manufacturer %></td>
      <td><%= phone.model %></td>
      <td><%= phone.description %></td>
      <td><%= phone.year %></td>
      <td><%= phone.resolution %></td>
      <td><%= phone.diagonal %></td>
      <td><%= phone.screen_type %></td>
      <td><%= phone.dpi %></td>
      <td><%= phone.multitouch %></td>
      <td><%= phone.cpu %></td>
      <td><%= phone.cores %></td>
      <td><%= phone.frequency %></td>
      <td><%= phone.gpu %></td>
      <td><%= phone.inner_mem %></td>
      <td><%= phone.access_mem %></td>
      <td><%= phone.ram %></td>
      <td><%= phone.cam %></td>
      <td><%= phone.megapixels %></td>
      <td><%= phone.photo_res %></td>
      <td><%= phone.hdr %></td>
      <td><%= phone.front_megapixels %></td>
      <td><%= phone.video_res %></td>
      <td><%= phone.fps %></td>
      <td><%= phone.four_k_video %></td>
      <td><%= phone.three_g %></td>
      <td><%= phone.four_g %></td>
      <td><%= phone.wifi %></td>
      <td><%= phone.bluetooth %></td>
      <td><%= phone.nfc %></td>
      <td><%= phone.gps %></td>
      <td><%= phone.gyroscope %></td>
      <td><%= phone.accelerometer %></td>
      <td><%= phone.barometer %></td>
      <td><%= phone.wireless_charge %></td>
      <td><%= phone.fast_charge %></td>
      <td><%= phone.vr %></td>
      <td><%= phone.battery %></td>
      <td><%= phone.standby_time %></td>
      <td><%= phone.talk_time %></td>
      <td><%= phone.os %></td>
      <td><%= phone.color %></td>
      <td><%= phone.weight %></td>
      <td><%= phone.dimensions %></td>
      <td><%= phone.material %></td>
      <td><%= phone.protection %></td>
      <td><%= link_to 'Show', phone_path(phone) %></td>
    </tr>
  <% end %>
</table>
