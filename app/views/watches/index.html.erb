      <td><%= watch.cores %></td>
      <td><%= watch.frequency %></td>
      <td><%= watch.gpu %></td>
      <td><%= watch.inner_mem %></td>
      <td><%= watch.access_mem %></td>
      <td><%= watch.ram %></td>
      <td><%= watch.cam %></td>
      <td><%= watch.megapixels %></td>
