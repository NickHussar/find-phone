  <% @watches.each do |watch| %>
    <tr>
      <td><%= watch.wt_developer %></td>
      <td><%= watch.wt_manufacturer %></td>
      <td><%= watch.wt_model %></td>
      <td><%= watch.wt_description %></td>
      <td><%= watch.wt_year %></td>
      <td><%= watch.wt_resolution %></td>
      <td><%= watch.wt_diagonal %></td>
      <td><%= watch.wt_color_screen %></td>
      <td><%= watch.wt_screen_type %></td>
      <td><%= watch.wt_dpi %></td>
