      <td><%= watch.wt_online_service %></td>
      <td><%= watch.wt_compatibility %></td>
      <td><%= watch.wt_speaker %></td>
      <td><%= watch.wt_microphone %></td>
      <td><%= watch.wt_color %></td>
      <td><%= watch.wt_weight %></td>
      <td><%= watch.wt_dimensions %></td>
      <td><%= watch.wt_material %></td>
      <td><%= watch.wt_protection %></td>
      <td><%= link_to 'Show', watch_path(watch) %></td>
      <td><%= link_to 'Edit', edit_watch_path(watch) %></td>
      <td><%= link_to 'Destroy', watch_path(watch),
              method: :delete,
              data: { confirm: 'Are you sure?' } %>
      </td>
    </tr>
  <% end %>
</table>
