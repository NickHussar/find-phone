      <td><%= watch.wt_bluetooth %></td>
      <td><%= watch.wt_nfc %></td>
      <td><%= watch.wt_gps %></td>
      <td><%= watch.wt_gyroscope %></td>
      <td><%= watch.wt_accelerometer %></td>
      <td><%= watch.wt_barometer %></td>
      <td><%= watch.wt_thermometer %></td>
      <td><%= watch.wt_compass %></td>
      <td><%= watch.wt_chronograph %></td>
      <td><%= watch.wt_pedometer %></td>
      <td><%= watch.wt_pulse_sensor %></td>
      <td><%= watch.wt_fitness_tracker %></td>
      <td><%= watch.wt_wireless_charge %></td>
      <td><%= watch.wt_fast_charge %></td>
      <td><%= watch.wt_battery %></td>
      <td><%= watch.wt_standby_time %></td>
      <td><%= watch.wt_clock_time %></td>
