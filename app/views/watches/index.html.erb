      <td><%= watch.wt_multitouch %></td>
      <td><%= watch.wt_cpu %></td>
      <td><%= watch.wt_cores %></td>
      <td><%= watch.wt_frequency %></td>
      <td><%= watch.wt_gpu %></td>
